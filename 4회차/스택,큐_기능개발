def solution(progresses, speeds):
    answer = []
    while progresses:   #progresses 리스트가 다 비워질 때까지
        result = 0
        while progresses and progresses[0] >= 100:   # 작업 리스트가 남아있고 맨 앞의 작업의 진도가 100인 경우:
            result+=1                                # 기능 배포 변수 증가. 
            progresses.pop(0)                        # 작업과 작업 속도를 리스트에서 제거
            speeds.pop(0)

        for i in range(len(progresses)):             # 작업 리스트의 진도를 증가
            progresses[i] = progresses[i]+speeds[i] 

        if result:                                   # 기능이 배포되면 결과에 추가
            answer.append(result)
    
    return answer

                     