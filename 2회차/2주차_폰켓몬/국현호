def solution(nums):
    x=int(len(nums)//2)
    
    #nums의 원소 종류 리스트    # s=set(nums)로 표현 가능하다.
    #nums.sort()
    #l=list() 
    #for i in range(0,nums[-1]+1):
    #    if i in nums:
    #        l.append(i)
    #    else:
    #        continue
    
    s=set(nums)
    
    # N/2 마리와 대소 비교
    if len(s)>x:
        return x
    else:
        return len(s)
